.SUFFIXES: .c .asm 

%.o: %.c
	@CC@ @CFLAGS@ -o $@ -c $<

%.o: %.asm
	@NASMPATH@ @NFLAGS@ -o $@ $<

ALL:	@ZSNESEXE@

@ZSNESEXE@:  ${OBJS}
	@CC@ -o @ZSNESEXE@ ${OBJS} @CFLAGS@ @LDFLAGS@

